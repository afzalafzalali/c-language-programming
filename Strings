//strings in c//
#include<stdio.h>
int main(){
char x[10];
printf("enter the string: ");
scanf("%s",&x);
printf("the string is : ",x);
return 0;
}

//function in strings//
//GETS AND PUTS//
#include<stdio.h>
int main(){
char x[10];
gets(x);
puts(x);
return 0;
}

//strrev//
#include<stdio.h>
int main(){
char x[10];
gets(x);
strrev(x);
return 0;
}
