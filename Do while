//print number from 1 to 10 using while loop//
#include<stdio.h>
int main(){
    int i=0;
    while(i++ < 10){
        printf("%d\n",i);
    }
    return 0;
}

//WAP to print sum of n digits using while loop//
#include<stdio.h>
int main(){
    int n,i=1,sum=0;
    printf("enter n\n");
    scanf("%d",&n);
    while(i<=n){
        sum = sum + i;
        i++;
    }
    printf("sum is %d",sum);
    return 0;
}

//wap to print the sum of digits in interval of 2//
#include<stdio.h>
int main(){
    int n,i=1,sum=0;
    printf("enter n\n");
    scanf("%d",&n);
    while(i<=n){
        sum = sum + i;
        i++;
    }
    printf("sum is %d",sum);
    return 0;
}

//find sum of its digits//
while(n!=0)
{
 d=n%10;
 n=n/10;
 s=s+d;
}
