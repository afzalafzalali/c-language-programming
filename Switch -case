//switch -case basic program//
#include<stdio.h>
int main(){
    int x;
    printf("enter x\n");
    scanf("%d",&x);
    switch(x){
        case 1:
        printf("jan");
        break;
        case 2:
        printf("feb");
        break;
        default:
        printf("invalid");
    }
    return 0;
}
