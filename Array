//array program//
#include<stdio.h>
int main(){
 int x[i],i;
  for(i=0;i<=9;i++){
    scanf("%d",&x[i]);
  }
return 0;

//how to input and output element in array//
#include<stdio.h>
int main(){
 int x[10];
 int i;
 printf("enter the elements")
  for(i=0;i<10;i++){
    scanf("%d",&x[i]);
  }
 for(i=0;i<10;i++){
    printf("%d",x[i]);
    printf("\n");
  }
return 0;
}
