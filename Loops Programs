//Basic for-loop program to print count 1 to 10//
#include<stdio.h>
int main(){
    int i;
    for(i=1;i<=10;printf("%d\n",i),i++);
    return 0;
}

//Print tables from 1 to 10 using for loop//
#include<stdio.h>
int main(){
    int i,j;
    for(i=1;i<=10;i++){
        for(j=1;j<=10;j++)
        {
            printf("%d * %d = %d\n\t",i,j,i*j);
        }
            printf("\t");
    }
    return 0;
}

//Program to print sum of digits of any number//
#include<stdio.h>
int main(){
    int s=0,n,d;
    printf("enter a number:");
    scanf("%d",&n);
    while(n!=0){
        d=n%10;
        n=n/10;
        s=s+d;
    }
    printf("sum is %d",s);
    return 0;
}
