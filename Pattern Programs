//Print Pattern using for loop//
//Pattern 1//
1
11
111
1111
11111

//code//
#include<stdio.h>
#include<conio.h>
int main(){
    int l,c;
    for(l=1;l<=5;l++){
        for(c=1;c<=l;c++)
        printf("1");
        printf("\n");
    }
    return 0;
}  

//Pattern 2//
1
12
123
1234
12345

//code//
#include<stdio.h>
#include<conio.h>
int main(){
    int l,c;
    for(l=1;l<=5;l++){
        for(c=1;c<=l;c++)
        printf("%d",c);
        printf("\n");
    }
    return 0;
} 

//Pattern 3//
1
22
333
4444
55555

//Code//
#include<stdio.h>
#include<conio.h>
int main(){
    int l,c;
    for(l=1;l<=5;l++){
        for(c=1;c<=l;c++)
        printf("%d",l);
        printf("\n");
    }
    return 0;
}   

//Pattern 4//
1
23
456
78910
1112131415

//code//
#include<stdio.h>
#include<conio.h>
int main(){
    int x=1,l,c;
    for(l=1;l<=5;l++){
        for(c=1;c<=l;c++){
        printf("%d",x);
        x++;
        }
        printf("\n");
    }
    return 0;
} 
