//array program//
#include<stdio.h>
int main(){
 int x[i],i;
  for(i=0;i<=9;i++){
    scanf("%d",&x[i]);
  }
return 0;

//how to input and output element in array//
#include<stdio.h>
int main(){
 int x[10];
 int i;
 printf("enter the elements")
  for(i=0;i<10;i++){
    scanf("%d",&x[i]);
  }
 for(i=0;i<10;i++){
    printf("%d",x[i]);
    printf("\n");
  }
return 0;
}

//reverse of array//
#include<stdio.h>
int main(){
 int x[10];
 int i;
 printf("enter the elements")
  for(i=0;i<10;i++){
    scanf("%d",&x[i]);
  }
 for(i=10;i>0;i--){
    printf("%d",x[i]);
    printf("\n");
  }
return 0;
}

//2D array//
#include<stdio.h>
int main(){
   /* 2D array declaration*/
   int disp[2][3];
   /*Counter variables for the loop*/
   int i, j;
   for(i=0; i<2; i++) {
      for(j=0;j<3;j++) {
         printf("Enter value for disp[%d][%d]:", i, j);
         scanf("%d", &disp[i][j]);
      }
   }
   //Displaying array elements
   printf("Two Dimensional array elements:\n");
   for(i=0; i<2; i++) {
      for(j=0;j<3;j++) {
         printf("%d ", disp[i][j]);
         if(j==2){
            printf("\n");
         }
      }
   }
   return 0;
}
