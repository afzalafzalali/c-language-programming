//switch -case basic program//
#include<stdio.h>
int main(){
    int x;
    printf("enter x\n");
    scanf("%d",&x);
    switch(x){
        case 1:
        printf("jan");
        break;
        case 2:
        printf("feb");
        break;
        default:
        printf("invalid");
    }
    return 0;
}

//Program to print grades using switch case//
#include<stdio.h>
int main(){
    int x;
    printf("enter x\n");
    scanf("%d",&x);
    switch(x/10){
        case 6:
        printf("D");
        break;
        case 7:
        printf("C");
        break;
        case 8:
        printf("B");
        break;
        case 9:
        printf("A");
        break;
        default:
        printf("invalid");
    }
    return 0;
}

//Program to find greatest of three numbers using switch case//
#include<stdio.h>
int main(){
    int x,y,z;
    printf("enter the values of  x y z\n");
    scanf("%d %d %d",&x,&y,&z);
    switch(x>y){
        case 1:
        switch(x>z){
            case 1:
            printf("x is greater");
            break;
            case 0:
            printf("z is greater");
        }
        break;
        default:
        if(y>z){
            printf("y is greater");
        }
        else{
            printf("z is greater");
        }
    }
    return 0;
}
